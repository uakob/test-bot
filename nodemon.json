{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "src/**/*.spec.ts"
  ],
  "verbose": true,
  "exec": "node --inspect=0.0.0.0:9229 --nolazy -r ./node_modules/ts-node/register -r ./node_modules/tsconfig-paths/register ./src/bootstrap.ts",
  "watch": [
    "src"
  ],
  "ext": "ts",
  "stdin": true,
  "stdout": true
}
